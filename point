class Point
{
int x;int y;
void setX(int x)
{
x=(x>79?79:(x<0?0:x));
}
void setY (int y)
{
y =(y>24?24:(y<0?0:y));
}
int getX()
{
return x;
}
int getY()
{
return y;
}
}
public class PointDemo
{
public static void main(String args[])
{
Point p1=new Point();
p1.x=-22;
p1.y=144;
System.out.println("x="+p1.x);

 System.out.println("x="+p1.y);
}
}
